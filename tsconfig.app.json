/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@app/*": ["src/app/*"]
    }
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
